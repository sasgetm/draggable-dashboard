  if (this.description.length < 2 || this.description.length > 64) {\n        this.descriptionError = 'Textarea input must be between 2 and 64 characters.';\n      } else {\n        this.descriptionError = '';\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n/* Add necessary styles */\nspan {\n  color: red;\n}\n</style>\n"],"mappings":"AA8BA,SAASA,QAAQ,EAAEC,UAAS,QAAS,MAAM;AAE3C,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,WAAW,EAAE,EAAE;MACfC,UAAU,EAAE,EAAE;MACdC,gBAAgB,EAAE;IACpB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACR,GAAGP,QAAQ,CAAC;MACV;IAAA,CACD,CAAC;IACFQ,YAAYA,CAAA,EAAG;MACb,OAAO,IAAI,CAACL,KAAK,CAACM,MAAK,IAAK,KAAK,IAAI,CAACN,KAAK,CAACM,MAAK,IAAK,EAAE;IAC1D,CAAC;IACDC,kBAAkBA,CAAA,EAAG;MACnB,OAAO,IAAI,CAACN,WAAW,CAACK,MAAK,IAAK,KAAK,IAAI,CAACL,WAAW,CAACK,MAAK,IAAK,EAAE;IACtE,CAAC;IACDE,gBAAgBA,CAAA,EAAG;MACjB,OAAO,CAAC,IAAI,CAACH,YAAW,IAAK,CAAC,IAAI,CAACE,kBAAkB;IACvD;EACF,CAAC;EACDE,OAAO,EAAE;IACP,GAAGX,UAAU,CAAC,CAAC,YAAY,CAAC,CAAC;IAC7BY,YAAYA,CAAA,EAAG;MACb,IAAI,CAAC,IAAI,CAACF,gBAAgB,EAAE;QAC1B,MAAMT,IAAG,GAAI;UACXC,KAAK,EAAE,IAAI,CAACA,KAAK;UACjBC,WAAW,EAAE,IAAI,CAACA;QACpB,CAAC;QACD,IAAI,CAACU,UAAU,CAACZ,IAAI,CAAC;QACrB,IAAI,CAACC,KAAI,GAAI,EAAE;QACf,IAAI,CAACC,WAAU,GAAI,EAAE;QACrB,IAAI,CAACC,UAAS,GAAI,EAAE;QACpB,IAAI,CAACC,gBAAe,GAAI,EAAE;MAC5B;IACF,CAAC;IACDS,aAAaA,CAAA,EAAG;MACd,IAAI,IAAI,CAACZ,KAAK,CAACM,MAAK,GAAI,KAAK,IAAI,CAACN,KAAK,CAACM,MAAK,GAAI,EAAE,EAAE;QACnD,IAAI,CAACJ,UAAS,GAAI,iDAAiD;MACrE,OAAO;QACL,IAAI,CAACA,UAAS,GAAI,EAAE;MACtB;IACF,CAAC;IACDW,mBAAmBA,CAAA,EAAG;MACpB,IAAI,IAAI,CAACZ,WAAW,CAACK,MAAK,GAAI,KAAK,IAAI,CAACL,WAAW,CAACK,MAAK,GAAI,EAAE,EAAE;QAC/D,IAAI,CAACH,gBAAe,GAAI,qDAAqD;MAC/E,OAAO;QACL,IAAI,CAACA,gBAAe,GAAI,EAAE;MAC5B;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}